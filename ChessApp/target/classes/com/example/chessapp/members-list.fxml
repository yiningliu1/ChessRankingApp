<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.chessapp.ControllerList">
    <children>
      <TableView fx:id="tbl_Rank" layoutX="19.0" layoutY="109.0" prefHeight="255.0" prefWidth="565.0">
        <columns>
          <TableColumn fx:id="col_rank" prefWidth="58.0" text="Rank" />
          <TableColumn fx:id="col_Name" prefWidth="420.0" text="Name" />
            <TableColumn fx:id="col_grade" prefWidth="86.0" text="Grade Level" />
        </columns>
      </TableView>
      <TextField fx:id="txt_Search" layoutX="157.0" layoutY="66.0" onKeyTyped="#filterList" prefHeight="25.0" prefWidth="427.0" promptText="Search by Name" />
      <Button fx:id="btn_AddMemberL" layoutX="19.0" layoutY="66.0" mnemonicParsing="false" onAction="#navAddMember" prefHeight="25.0" prefWidth="118.0" text="Add Member" />
      <Button fx:id="btn_AddGame" layoutX="19.0" layoutY="21.0" mnemonicParsing="false" onAction="#navAddGame" prefHeight="25.0" prefWidth="94.0" text="Add Game" />
      <Button fx:id="btn_ViewGames" layoutX="490.0" layoutY="21.0" mnemonicParsing="false" onAction="#navViewGames" prefHeight="25.0" prefWidth="94.0" text="View Games" />
      <Button fx:id="btn_EditMember" layoutX="388.0" layoutY="21.0" mnemonicParsing="false" onAction="#navEditMember" prefHeight="25.0" prefWidth="92.0" text="Edit Member" />
    </children>
</AnchorPane>
